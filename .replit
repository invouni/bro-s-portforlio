run = "vite"

modules = ["nodejs-20"]
hidden = [".config", "package-lock.json"]

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_05"

[unitTest]
language = "nodejs"

[deployment]

deploymentTarget = "cloudrun"
ignorePorts = false

[[ports]]
localPort = 5173
externalPort = 5173
exposeLocalhost = true
